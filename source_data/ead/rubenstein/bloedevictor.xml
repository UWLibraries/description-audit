               <date>1983</date>
               <eventgrp>
                  <event>Retired from Benton &amp;amp; Bowles</event>
                  <event>
                     <title type="simple" render="italic">The Full Service Advertising
                        Agency</title> published</event>
               </eventgrp>
            </chronitem>
