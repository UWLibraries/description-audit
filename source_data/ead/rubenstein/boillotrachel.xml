      <c01 id="aspace_73901688588ea159714a56196ebb53a5" level="series">
        <did>
          <unittitle>Moon Shine</unittitle>
          <physdesc altrender="whole"><extent altrender="materialtype spaceoccupied">3.0 Linear
              Feet</extent><extent altrender="carrier">2 boxes</extent><physfacet>62 inkjet
