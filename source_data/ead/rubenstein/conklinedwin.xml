      <bioghist id="aspace_50e0012bfcc3f929c7631cb2346412b7">
         <head>Biographical Note</head>
         <chronlist>
            <head>Chronology List</head>
            <chronitem>
               <date>1863, Nov. 24</date>
               <eventgrp>
                  <event>Born, Waldo, Ohio</event>
               </eventgrp>
            </chronitem>
            <chronitem>
